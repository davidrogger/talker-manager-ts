{
  "env": {
    "node": true,
    "jest": true,
    "browser": true
  },
  "plugins": ["sonarjs"],
  "extends": [
    "next/core-web-vitals",
    "airbnb-base",
    "plugin:jest-dom/recommended",
    "plugin:testing-library/react"
  ],
  "rules": {
    "import/extensions": ["error", "ignorePackages", {
      "js": "never",
      "jsx": "never",
      "ts": "never",
      "tsx": "never",
      "mjs": "never"
    }]
  }
}

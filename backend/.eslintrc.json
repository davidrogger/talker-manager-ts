{
    "env": {
        "es2021": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base",
        "airbnb-typescript/base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "rules": {
        "no-console": "off",
        "@typescript-eslint/no-unused-vars": ["error", { "args": "none" }]
    }
}
